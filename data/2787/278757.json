atus":"Air"},{"id":925402,"url":"http://bgm.tv/ep/925402","type":0,"sort":4,"name":"何事も真剣に楽しんでる人には敵わない","name_cn":"","duration":"","airdate":"2020-01-29","comment":2,"desc":"ラグビー部へと復帰した伊吹を加え、威翠大学ラグビー部との試合へ臨む堂紫社大学ラグビー部のメンバーたち。\r\nそこへ夏紗のファンだという相手チームのウイング・日吉琉星が現れる。\r\n有名人扱いをされ、すっかり上機嫌になる夏紗。\r\nそんな中、リザーブの伊吹が交代で試合に参加するものの、靖也は琉星のプレーに翻弄され……。","status":"Air"},{"id":925403,"url":"http://bgm.tv/ep/925403","type":0,"sort":5,"name":"","name_cn":"","duration":"","airdate":"2020-02-05","comment":2,"desc":"","status":"Air"},{"id":925404,"url":"http://bgm.tv/ep/925404","type":0,"sort":6,"name":"","name_cn":"","duration":"","airdate":"2020-02-12","comment":1,"desc":"","status":"Air"},{"id":925405,"url":"http://bgm.tv/ep/925405","type":0,"sort":7,"name":"","name_cn":"","duration":"","airdate":"2020-02-19","comment":2,"desc":"","status":"Air"},{"id":925406,"url":"http://bgm.tv/ep/925406","type":0,"sort":8,"name":"","name_cn":"","duration":"","airdate":"2020-02-26","comment":0,"desc":"","status":"NA"},{"id":925407,"url":"http://bgm.tv/ep/925407","type":0,"sort":9,"name":"","name_cn":"","duration":"","airdate":"2020-03-04","comment":0,"desc":"","status":"NA"},{"id":925408,"url":"http://bgm.tv/ep/925408","type":0,"sort":10,"name":"","name_cn":"","duration":"","airdate":"2020-03-11","comment":0,"desc":"","status":"NA"},{"id":925409,"url":"http://bgm.tv/ep/925409","type":0,"sort":11,"name":"","name_cn":"","duration":"","airdate":"2020-03-18","comment":0,"desc":"","status":"NA"},{"id":925410,"url":"http://bgm.tv/ep/925410","type":0,"sort":12,"name":"","name_cn":"","duration":"","airdate":"2020-03-25","comment":0,"desc":"","status":"NA"}],"crt":[{"id":71153,"image":"//lain.bgm.tv/pic/crt/g/de/05/71153_crt_5W0z8.jpg","name":"柚木夏纱","desc":"河西健吾"},{"id":71154,"image":"//lain.bgm.tv/pic/crt/g/c3/40/71154_crt_kPGrX.jpg","name":"真行寺清一郎","desc":"鈴木崚汰"},{"id":71155,"image":"//lain.bgm.tv/pic/crt/g/22/5d/71155_crt_chhDC.jpg","name":"上丘伊吹","desc":"柳田淳一"},{"id":71156,"image":"//lain.bgm.tv/pic/crt/g/a3